{
	"name": "mathematicator-core/tokenizer",
	"description": "Math Tokenizer",
	"homepage": "https://github.com/mathematicator-core/tokenizer",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": ">=7.2",
		"nette/utils": "^3.0",
		"nette/tokenizer": "^3.0",
		"mathematicator-core/numbers": "^2.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.18",
		"tracy/tracy": "^2.7",
		"phpstan/phpstan-nette": "^0.12.6",
		"nette/bootstrap": "^3.0.1",
		"nette/tester": "^2.0"
	},
	"suggest": {
		"nette/di": "To use package in Nette Framework"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 6 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
